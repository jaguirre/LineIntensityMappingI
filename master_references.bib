@inproceedings{Schlaerth_10,
Author = {Schlaerth, James A. and Czakon, Nicole G. and Day, Peter K. and Downes,
   Thomas P. and Duan, Ran and Gao, Jiansong and Glenn, Jason and Golwala,
   Sunil R. and Hollister, Matthew I. and LeDuc, Henry G. and Mazin,
